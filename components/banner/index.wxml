<!--components/banner/index.wxml-->
<view class="banner">
	<swiper indicator-dots="true" autoplay="true"  circular="true">
		<block wx:for="{{background}}" wx:key="*this">
			<swiper-item>
				<view class="swiper-item {{item}}">
					<image class="cover" src="{{item.thumb}}" mode="aspectFill"></image>
				</view>
			</swiper-item>
		</block>
	</swiper>
</view>